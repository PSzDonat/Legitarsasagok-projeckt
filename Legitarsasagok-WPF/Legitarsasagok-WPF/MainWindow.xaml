        <Grid Background="White" Height="457" Width="858">
            <Frame Name="Page1" Source="/Pages/Page1.xaml" Visibility="{Binding Visibility_Page1}"/>
            <Frame Name="Page2" Source="/Pages/Page2.xaml" Visibility="{Binding Visibility_Page2}"/>
            <Frame Name="Page3" Source="/Pages/Page3.xaml" Visibility="{Binding Visibility_Page3}"/>
            <Frame Name="Page4" Source="/Pages/Page4.xaml" Visibility="{Binding Visibility_Page4}"/>
        </Grid>
    </Viewbox>
</Window>
