        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Legitarsasagok_WPF"
        mc:Ignorable="d"
        Title="Repülő járatok" Height="585" Width="1040" Background="Black" Topmost="False" WindowState="Normal" WindowStartupLocation="CenterScreen">
